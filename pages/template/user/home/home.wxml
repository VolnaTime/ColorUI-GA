<!--pages/template/user/home/home.wxml-->
<import src="/pages/template/common/copyright.wxml" />

<cu-custom bgColor="bg-white" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">个人中心UI设计</view>
</cu-custom>

<!-- 页面 -->
<scroll-view scroll-y class="scrollPage cu-timeline bg-gray">
  <view class='nav-list margin-top'>
    <navigator open-type="navigate" hover-class='none' url="/pages/template/user/{{item.name}}/{{item.name}}" class="nav-lie" wx:for="{{elements}}" wx:key="item">
      <image src='{{item.url}}' mode='widthFix' class='image-bg radius-lg'></image>
    </navigator>
  </view>
  <template is="copyright"></template>
  <view class="cu-tabbar-height"></view>
</scroll-view>